KocatanTailor_fey_dirhai = de "Der Fey Dirhai"
KocatanTailor_fey_dirhai_r = de "Sie glitzern blau."
KocatanTailor_avar = de "Avar-Kämpfer"
KocatanTailor_avar_r = de "Toll."
