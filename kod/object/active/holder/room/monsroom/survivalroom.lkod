boss_goal_msg = de "Einen Boss töten."
minibosses_goal_msg = de "%i Minibosse töten."
boss_slain_goal_msg = de "~BEin Boss wurde getötet!"
minibosses_slain_goal_msg = de "~BZwei Minibosse wurden getötet!"
kills_remaining = de \
   "Es sind noch %i kills nötig um die nächste Welle zu erreichen."
extralife_header = de "Um ein extra Leben zu bekommen müsst ihr:"
kill_friendly_msg = de "Einen befreundeten Mitstreiter töten."
player_murdered_player_no_goal = de "~B%s wurde von %s getötet!"
player_murdered_player_goal = de "~B%s wurde durch %s geopfert!"
