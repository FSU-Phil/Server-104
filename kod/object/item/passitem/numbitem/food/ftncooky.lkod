fortunecookie_name_rsc = de "Der Glückskeks"
fortunecookie_desc_rsc = de \
   "Der Wunsch nach einer glücklichen Zukunft geht einher mit dem Wunsch "
   "nach einem süßen Keks. Solange Du den Glückskeks nicht aufbrichst und "
   "Deine Nachricht liest, werden beide Wünsche unerfüllt bleiben."
fortunecookie_name_plural_rsc = de "Glückskekse"
fortunecookie_disp_name = de "Ein Korb voller Glückskekse"
fortunecookie_disp_desc = de \
