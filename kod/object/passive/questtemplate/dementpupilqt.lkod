dementpupilquest_name_rsc = de "Verblöden Schüler Quest"
dementpupilquest_desc_rsc = de \
   "Afiera D'xor hat dich beauftragt ein paar Güter zu überbringen. Erledige es, "
   "und kehre zurück zu ihm, wenn Du fertig bist."
dementpupilquest_nodetwo_rsc = de \
   "Afiera D'xor hat dich beauftragt %s%s zu %s zu überbringen."
